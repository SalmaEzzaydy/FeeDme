{"ast":null,"code":"import axios from \"axios\";\nimport swal from \"sweetalert\";\nexport default {\n  props: [\"baseURL\"],\n  data() {\n    return {\n      email: null,\n      firstName: null,\n      lastName: null,\n      password: null,\n      confirmPassword: null\n    };\n  },\n  methods: {\n    async signup(e) {\n      e.preventDefault();\n      if (this.password === this.confirmPassword) {\n        // call signup api\n        const user = {\n          email: this.email,\n          firstName: this.firstName,\n          lastName: this.lastName,\n          password: this.password\n        };\n        console.log(\"user\", user);\n        await axios.post(`${this.baseURL}user/signup`, user).then(() => {\n          this.$router.replace(\"/\");\n          swal({\n            text: \"User signup successful, please login\",\n            icon: \"success\"\n          });\n        }).catch(err => console.log(\"err\", err));\n      } else {\n        // show some error\n        swal({\n          text: \"passwords dont match\",\n          icon: \"error\"\n        });\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAiFA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,YAAY;AAC7B,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAE,IAAI;MACdC,QAAQ,EAAE,IAAI;MACdC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,MAAM,CAACC,CAAC,EAAE;MACdA,CAAC,CAACC,cAAc,EAAE;MAClB,IAAI,IAAI,CAACL,QAAO,KAAM,IAAI,CAACC,eAAe,EAAE;QAC1C;QACA,MAAMK,IAAG,GAAI;UACXT,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,SAAS,EAAE,IAAI,CAACA,SAAS;UACzBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC;QACDO,OAAO,CAACC,GAAG,CAAC,MAAM,EAAEF,IAAI,CAAC;QACzB,MAAMb,KAAI,CACPgB,IAAI,CAAE,GAAE,IAAI,CAACC,OAAQ,aAAY,EAAEJ,IAAI,EACvCK,IAAI,CAAC,MAAM;UACV,IAAI,CAACC,OAAO,CAACC,OAAO,CAAC,GAAG,CAAC;UACzBnB,IAAI,CAAC;YACHoB,IAAI,EAAE,sCAAsC;YAC5CC,IAAI,EAAE;UACR,CAAC,CAAC;QACJ,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAKV,OAAO,CAACC,GAAG,CAAC,KAAK,EA