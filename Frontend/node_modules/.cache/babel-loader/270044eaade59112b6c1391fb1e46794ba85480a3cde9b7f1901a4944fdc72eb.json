{"ast":null,"code":"import axios from \"axios\";\nimport swal from \"sweetalert\";\nexport default {\n  props: [\"baseURL\"],\n  data() {\n    return {\n      email: null,\n      password: null\n    };\n  },\n  methods: {\n    async signin(e) {\n      e.preventDefault();\n      const body = {\n        email: this.email,\n        password: this.password\n      };\n      await axios.post(`${this.baseURL}user/signIn`, body).then(res => {\n        this.$router.replace(\"/\");\n        localStorage.setItem(\"token\", res.data.token);\n        swal({\n          text: \"Login successful\",\n          icon: \"success\"\n        });\n      }).catch(err => console.log(\"err\", err));\n    }\n  }\n};","map":{"version":3,"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,YAAY;AAC7B,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,IAAI;MACXC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,MAAM,CAACC,CAAC,EAAE;MACdA,CAAC,CAACC,cAAc,EAAE;MAClB,MAAMC,IAAG,GAAI;QACXN,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;MACD,MAAML,KAAI,CACPW,IAAI,CAAE,GAAE,IAAI,CAACC,OAAQ,aAAY,EAAEF,IAAI,EACvCG,IAAI,CAAEC,GAAG,IAAK;QACb,IAAI,CAACC,OAAO,CAACC,OAAO,CAAC,GAAG,CAAC;QACzBC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEJ,GAAG,CAACX,IAAI,CAACgB,KAAK,CAAC;QAC7ClB,IAAI,CAAC;UACHmB,IAAI,EAAE,kBAAkB;UACxBC,IAAI,EAAE;QACR,CAAC,CAAC;MACJ,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,GAAG,CAAC,CAAC;IAC5C;EACF;AACF,CAAC","names":["axios","swal","props","data","email","password","methods","signin","e","preventDefault","body","post","baseURL","then","res","$router","replace","localStorage","setItem","token","text","icon","catch","err","console","log"],"sourceRoot":"","sources":["C:\\Users\\lenovo\\Desktop\\vue\\ecommerce-ui\\src\\views\\Signin.vue"],"sourcesContent":["<template>\n  <div class=\"conatiner\">\n    <div class=\"row\">\n      <div class=\"col-12 justify-content-center d-flex flex-row pt-5\">\n        <div id=\"signin\" class=\"flext-item border\">\n          <h2 class=\"pt-4\">Sign-In</h2>\n          <form @submit=\"signin\" class=\"form-group pt-4 pl-4 pr-4\">\n            <div class=\"form-group\">\n              <label>Email </label>\n              <input v-model=\"email\" type=\"email\" class=\"form-control\" />\n            </div>\n            <div class=\"form-group\">\n              <label>Password </label>\n              <input v-model=\"password\" type=\"password\" class=\"form-control\" />\n            </div>\n            <button class=\"btn btn-primary mt-2 py-0\">Continue</button>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nimport swal from \"sweetalert\";\nexport default {\n  props: [\"baseURL\"],\n  data() {\n    return {\n      email: null,\n      password: null,\n    };\n  },\n  methods: {\n    async signin(e) {\n      e.preventDefault();\n      const body = {\n        email: this.email,\n        password: this.password,\n      };\n      await axios\n        .post(`${this.baseURL}user/signIn`, body)\n        .then((res) => {\n          this.$router.replace(\"/\");\n          localStorage.setItem(\"token\", res.data.token);\n          swal({\n            text: \"Login successful\",\n            icon: \"success\",\n          });\n        })\n        .catch((err) => console.log(\"err\", err));\n    },\n  },\n};\n</script>\n<style scoped>\n.btn-primary {\n  background-color: #f0c14b;\n  color: black;\n}\n\n@media screen and (min-width: 992px) {\n  #signin {\n    width: 40%;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}