{"ast":null,"code":"import swal from \"sweetalert\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      product: {},\n      category: {},\n      wishListString: \"Add to wishlist\"\n    };\n  },\n  props: [\"baseURL\", \"products\", \"categories\"],\n  methods: {\n    addToWishlist() {\n      if (!this.token) {\n        // user is not logged in\n        // show some error\n        swal({\n          text: \"please login to add item in wishlist\",\n          icon: \"error\"\n        });\n        return;\n      }\n      // add item to wishlist\n      axios.post(`${this.baseURL}wishlist/add?token=${this.token}`, {\n        id: this.product.id\n      }).then(res => {\n        if (res.status === 201) {\n          this.wishListString = \"Added to Wishlist\";\n          swal({\n            text: \"Added to Wishlist\",\n            icon: \"success\"\n          });\n        }\n      }).catch(err => {\n        console.log(\"err\", err);\n      });\n    }\n  },\n  mounted() {\n    this.id = this.$route.params.id;\n    this.product = this.products.find(product => product.id == this.id);\n    this.category = this.categories.find(category => category.id == this.product.categoryId);\n    this.token = localStorage.getItem(\"token\");\n  }\n};","map":{"version":3,"mappings":"AAsCA,OAAOA,IAAG,MAAO,YAAY;AAC7B,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,CAAC,CAAC;MACXC,QAAQ,EAAE,CAAC,CAAC;MACZC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,KAAK,EAAE,CAAC,SAAS,EAAE,UAAU,EAAE,YAAY,CAAC;EAC5CC,OAAO,EAAE;IACPC,aAAa,GAAG;MACd,IAAI,CAAC,IAAI,CAACC,KAAK,EAAE;QACf;QACA;QACAT,IAAI,CAAC;UACHU,IAAI,EAAE,sCAAsC;UAC5CC,IAAI,EAAE;QACR,CAAC,CAAC;QACF;MACF;MACA;MACAV,KAAI,CACDW,IAAI,CAAE,GAAE,IAAI,CAACC,OAAQ,sBAAqB,IAAI,CAACJ,KAAM,EAAC,EAAE;QACvDK,EAAE,EAAE,IAAI,CAACX,OAAO,CAACW;MACnB,CAAC,EACAC,IAAI,CAAEC,GAAG,IAAK;QACb,IAAIA,GAAG,CAACC,MAAK,KAAM,GAAG,EAAE;UACtB,IAAI,CAACZ,cAAa,GAAI,mBAAmB;UACzCL,IAAI,CAAC;YACHU,IAAI,EAAE,mBAAmB;YACzBC,IAAI,EAAE;UACR,CAAC,CAAC;QACJ;MACF,CAAC,EACAO,KAAK,CAAEC,GAAG,IAAK;QACdC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,GAAG,CAAC;MACzB,CAAC,CAAC;IACN;EACF,CAAC;EACDG,OAAO,GAAG;IACR,IAAI,CAACR,EAAC,GAAI,IAAI,CAACS,MAAM,CAACC,MAAM,CAACV,EAAE;IAC/B,IAAI,CAACX,OAAM,GAAI,IAAI,CAACsB,QAAQ,CAACC,IAAI,CAAEvB,OAAO,IAAKA,OAAO,CAACW,EAAC,IAAK,IAAI,CAACA,EAAE,CAAC;IACrE,IAAI,CAACV,QAAO,GAAI,IAAI,CAACuB,UAAU,CAACD,IAAI,CACjCtB,QAAQ,IAAKA,QAAQ,CAACU,EAAC,IAAK,IAAI,CAACX,OAAO,CAACyB,UAAS,CACpD;IACD,IAAI,CAACnB,KAAI,GAAIoB,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;EAC5C;AACF,CAAC","names":["swal","axios","data","product","category","wishListString","props","methods","addToWishlist","token","text","icon","post","baseURL","id","then","res","status","catch","err","console","log","mounted","$route","params","products","find","categories","categoryId","localStorage","getItem"],"sourceRoot":"","sources":["C:\\Users\\lenovo\\Desktop\\vue\\ecommerce-ui\\src\\views\\Product\\ShowDetails.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row pt-5\">\n      <div class=\"col-md-1\"></div>\n      <!--            display image-->\n      <div class=\"col-md-4 col-12\">\n        <img :src=\"product.imageURL\" class=\"img-fluid\" />\n      </div>\n      <!--            display product details-->\n      <div class=\"col-md-6 col-12 pt-3 pt-md-0\">\n        <h4>{{ product.name }}</h4>\n        <h6 class=\"catgory font-italic\">{{ category.categoryName }}</h6>\n        <h6 class=\"font-weight-bold\">$ {{ product.price }}</h6>\n        <p>\n          {{ product.description }}\n        </p>\n        <div class=\"features pt-3\">\n          <h5><strong>Features</strong></h5>\n          <ul>\n            <li>Lorem ipsum dolor sit amet consectetur adipisicing elit.</li>\n            <li>Officia quas, officiis eius magni error magnam voluptatem</li>\n            <li>nesciunt quod! Earum voluptatibus quaerat dolorem doloribus</li>\n            <li>molestias ipsum ab, ipsa consectetur laboriosam soluta et</li>\n            <li>ut doloremque dolore corrupti, architecto iusto beatae.</li>\n          </ul>\n        </div>\n        <button\n          id=\"wishlist-button\"\n          class=\"btn mr-3 p-1 py-0\"\n          @click=\"addToWishlist()\"\n        >\n          {{ wishListString }}\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport swal from \"sweetalert\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      product: {},\n      category: {},\n      wishListString: \"Add to wishlist\",\n    };\n  },\n  props: [\"baseURL\", \"products\", \"categories\"],\n  methods: {\n    addToWishlist() {\n      if (!this.token) {\n        // user is not logged in\n        // show some error\n        swal({\n          text: \"please login to add item in wishlist\",\n          icon: \"error\",\n        });\n        return;\n      }\n      // add item to wishlist\n      axios\n        .post(`${this.baseURL}wishlist/add?token=${this.token}`, {\n          id: this.product.id,\n        })\n        .then((res) => {\n          if (res.status === 201) {\n            this.wishListString = \"Added to Wishlist\";\n            swal({\n              text: \"Added to Wishlist\",\n              icon: \"success\",\n            });\n          }\n        })\n        .catch((err) => {\n          console.log(\"err\", err);\n        });\n    },\n  },\n  mounted() {\n    this.id = this.$route.params.id;\n    this.product = this.products.find((product) => product.id == this.id);\n    this.category = this.categories.find(\n      (category) => category.id == this.product.categoryId\n    );\n    this.token = localStorage.getItem(\"token\");\n  },\n};\n</script>\n<style>\n.category {\n  font-weight: 400;\n}\n\n#wishlist-button {\n  background-color: #b9b9b9;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}