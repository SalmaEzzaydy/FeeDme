{"ast":null,"code":"import ProductBox from '../../components/Product/ProductBox';\nexport default {\n  name: 'ListProducts',\n  data() {\n    return {\n      id: null,\n      categoryIndex: null,\n      category: {},\n      len: 0,\n      msg: null\n    };\n  },\n  components: {\n    ProductBox\n  },\n  props: [\"baseURL\", \"categories\"],\n  mounted() {\n    this.id = this.$route.params.id;\n    this.categoryIndex = this.categories.findIndex(category => category.id == this.id);\n    this.category = this.categories[this.categoryIndex];\n    this.len = this.category.products.length;\n    console.warn(this.len);\n    if (this.len == 0) {\n      this.msg = \"Sorry, no products found\";\n    } else if (this.len == 1) {\n      this.msg = \"Only 1 product found\";\n    } else {\n      this.msg = this.len + \" products found\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAmBA,OAAOA,UAAS,MAAO,qCAAqC;AAC5D,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAI,GAAE;IACJ,OAAO;MACLC,EAAC,EAAI,IAAI;MACTC,aAAY,EAAI,IAAI;MACpBC,QAAO,EAAI,CAAC,CAAC;MACbC,GAAE,EAAI,CAAC;MACPC,GAAE,EAAI;IACR;EACF,CAAC;EACDC,UAAS,EAAI;IAACR;EAAU,CAAC;EACzBS,KAAI,EAAI,CAAE,SAAQ,EAAI,YAAW,CAAG;EACpCC,OAAO,GAAG;IACR,IAAI,CAACP,EAAC,GAAI,IAAI,CAACQ,MAAM,CAACC,MAAM,CAACT,EAAE;IAC/B,IAAI,CAACC,aAAY,GAAI,IAAI,CAACS,UAAU,CAACC,SAAS,CAACT,QAAO,IAAKA,QAAQ,CAACF,EAAC,IAAK,IAAI,CAACA,EAAE,CAAC;IAClF,IAAI,CAACE,QAAO,GAAI,IAAI,CAACQ,UAAU,CAAC,IAAI,CAACT,aAAa,CAAC;IAEnD,IAAI,CAACE,GAAE,GAAI,IAAI,CAACD,QAAQ,CAACU,QAAQ,CAACC,MAAM;IACxCC,OAAO,CAACC,IAAI,CAAC,IAAI,CAACZ,GAAG,CAAC;IACtB,IAAG,IAAI,CAACA,GAAE,IAAK,CAAC,EAAE;MAChB,IAAI,CAACC,GAAE,GAAI,0BAA0B;IACvC,OAAO,IAAG,IAAI,CAACD,GAAE,IAAK,CAAC,EAAE;MACvB,IAAI,CAACC,GAAE,GAAI,sBAAsB;IACnC,OAAO;MACL,IAAI,CAACA,GAAE,GAAI,IAAI,CAACD,GAAE,GAAI,iBAAiB;IACzC;EACF;AACF","names":["ProductBox","name","data","id","categoryIndex","category","len","msg","components","props","mounted","$route","params","categories","findIndex","products","length","console","warn"],"sourceRoot":"","sources":["C:\\Users\\lenovo\\Desktop\\vue\\ecommerce-ui\\src\\views\\Category\\ListProducts.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-12 text-center\">\n        <h4 class=\"pt-3\">{{category.categoryName}}</h4>\n        <h5>{{msg}}</h5>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div v-for=\"product of category.products\" :key=\"product.id\" class=\"col-md-6 col-xl-4 col-12 pt-3  justify-content-around d-flex\">\n        <ProductBox :product=\"product\">\n        </ProductBox>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ProductBox from '../../components/Product/ProductBox';\nexport default {\n  name: 'ListProducts',\n  data(){\n    return {\n      id : null,\n      categoryIndex : null,\n      category : {},\n      len : 0,\n      msg : null\n    }\n  },\n  components : {ProductBox},\n  props : [ \"baseURL\" , \"categories\" ],\n  mounted() {\n    this.id = this.$route.params.id;\n    this.categoryIndex = this.categories.findIndex(category => category.id == this.id);\n    this.category = this.categories[this.categoryIndex];\n\n    this.len = this.category.products.length;\n    console.warn(this.len);\n    if(this.len == 0) {\n      this.msg = \"Sorry, no products found\";\n    } else if(this.len == 1) {\n      this.msg = \"Only 1 product found\";\n    } else {\n      this.msg = this.len + \" products found\";\n    }\n  }\n}\n</script>\n\n<style scoped>\nh4 {\n  font-family: 'Roboto', sans-serif;\n  color: #484848;\n  font-weight: 700;\n}\n\nh5 {\n  font-family: 'Roboto', sans-serif;\n  color: #686868;\n  font-weight: 300;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}